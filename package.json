    "jsonfile": "^6.0.1",
    "keycode": "^2.2.0",
    "meow": ">=7.0.1",
    "minimist": ">=1.2.5",
    "minimist-options": ">=4.1.0",
    "moment": ">=2.27.0",
    "newman": ">=5.1.0",
    "node-cmd": ">=3.0.0",
    "npm-path": ">=2.0.4",
    "path": ">=0.12.7",
    "pdf-parse": ">=1.1.1",
    "python-shell": "^2.0.1",
    "request": ">=2.88.2",
    "robotjs": ">=0.6.0",
    "shell-quote": "^1.7.2",
    "strip-ansi": "^6.0.0",
    "testrail-api": "^1.3.6",
    "url-parse": ">=1.4.7",
