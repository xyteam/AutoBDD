{
  "name": "autobdd",
  "version": "2.0.0",
  "description": "BDD Automaiton",
  "scripts": {
    "postinstall": "npm run e2e-init",
    "e2e-init": ". .autoPathrc.sh; cd ./test-projects/autobdd-test/e2e-test/test-init; npx wdio abdd.js",
    "e2e-autorunner": ". .autoPathrc.sh; rm -rf test-projects/autobdd-test/test-results/build-test; autorunner.py --project autobdd-test --reportpath build-test --movie 1 -- --cucumberOpts.tags='@Init and not @Report'; cat test-projects/autobdd-test/test-results/build-test/e2e-test/*/*.run",
    "check-e2e-results": ". .autoPathrc.sh; cd ./test-projects/autobdd-test/e2e-test/check-report; npx wdio abdd.js",
    "jest-test": ". .autoPathrc.sh; cd ./test-projects/autobdd-test; jest --verbose js-code/",
    "py-test": ". .autoPathrc.sh; cd ./test-projects/autobdd-test; python -m pytest -r A py-code/",
    "cy-test": ". .autoPathrc.sh; cd ./test-projects/autobdd-test/cal-app; npm install; npm start; cypress run; npm stop",
    "unit-test": "npm run jest-test & npm run py-test",
    "int-test": "npm run cy-test",
    "e2e-test": "npm run e2e-autorunner && npm run check-e2e-results",
    "test": "npm run unit-test && npm run int-test && npm run e2e-test"
  },
  "dependencies": {
    "ajv": "^6.12.3",
    "assert": "^2.0.0",
    "bottleneck": ">=2.19.5",
    "child_process": ">=1.0.2",
    "cryptiles": ">=4.1.3",
    "deep-extend": ">=0.6.0",
    "encodeurl": ">=1.0.2",
    "fs-ext": "^2.0.0",
    "fuzzball": "^1.3.0",
    "glob": ">=7.1.6",
    "hapi": ">=18.1.0",
    "hoek": ">=6.1.3",
    "java": "^0.12.1",
    "json-diff": ">=0.5.4",
    "jsonfile": "^6.0.1",
    "keycode": "^2.2.0",
    "meow": ">=7.0.1",
    "minimist": ">=1.2.5",
    "minimist-options": ">=4.1.0",
    "moment": ">=2.27.0",
    "newman": ">=5.1.2",
    "node-cmd": ">=3.0.0",
    "npm-path": ">=2.0.4",
    "path": ">=0.12.7",
    "pdf-parse": ">=1.1.1",
    "python-shell": "^2.0.1",
    "request": ">=2.88.2",
    "robotjs": ">=0.6.0",
    "shell-quote": "^1.7.2",
    "strip-ansi": "^6.0.0",
    "testrail-api": "^1.3.6",
    "url-parse": ">=1.4.7",
    "uuid-random": "^1.3.2",
    "words-to-numbers": ">=1.5.1",
    "xlsx": "^0.16.4",
    "xysikulixapi": "git+https://github.com/xyteam/xySikulixApi.git"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:xyteam/AutoBDD.git"
  },
  "keywords": [
    "AutoBDD",
    "end-to-end",
    "e2e test",
    "unit test",
    "cucumber",
    "webdriverio",
    "jest",
    "pytest",
    "cypress",
    "automation",
    "framework",
    "allure"
  ],
  "author": "xyteam",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/register": "^7.10.5",
    "@rpii/wdio-html-reporter": "^6.0.7",
    "@shelex/cypress-allure-plugin": "^1.8.0",
    "@wdio/allure-reporter": "^6.3.0",
    "@wdio/cli": "^6.3.5",
    "@wdio/cucumber-framework": "^6.3.0",
    "@wdio/dot-reporter": "^6.3.0",
    "@wdio/jasmine-framework": "^6.3.5",
    "@wdio/local-runner": "^6.3.5",
    "@wdio/selenium-standalone-service": "^6.1.14",
    "@wdio/spec-reporter": "^6.3.0",
    "@wdio/sync": "^6.3.3",
    "allure-commandline": "^2.13.0",
    "chromedriver": "^84.0.1",
